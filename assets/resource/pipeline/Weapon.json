    "WeaponUpgradeSetFilter": {
        "desc": "设置武器库筛选条件",
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": [
                    "武器"
                ],
                "roi": [
                    0,
                    0,
                    155,
                    72
                ]
            }
        },
        "pre_delay": 0,
        "post_delay": 0,
        "post_wait_freezes": 1,
