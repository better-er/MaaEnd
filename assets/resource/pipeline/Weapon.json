    "WeaponUpgradeSelectLowLevelWeapons": {
        "desc": "选择低等级武器详情",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "threshold": 0.9,
                "template": "Weapon/WeaponDetails.png",
                "roi": [
                    985,
                    591,
                    277,
                    112
                ]
            }
        },
        "action": {
            "type": "Click"
        },
        "post_wait_freezes": 1,
        "next": [
            "WeaponUpgradeClickLevelUpButton"
        ]
