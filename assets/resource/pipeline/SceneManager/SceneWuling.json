{
    "ScenePrivateWorldEnterMapWulingWulingCity": {
        "doc": "在大世界界面，进入武陵-武陵城地图",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    -200,
                    0,
                    200,
                    200
                ],
                "template": [
                    "SceneManager/WorldMenu.png"
                ],
                "green_mask": true
            }
        },
        "action": {
            "type": "ClickKey",
            "param": {
                "key": 77 // M
            }
        },
        "post_wait_freezes": 400,
        "next": [
            "ScenePrivateMapAnyEnterMapWulingWulingCitySuccess",
            "ScenePrivateMapOverviewEnterMapWulingWulingCity",
            "[JumpBack]ScenePrivateMapAnyEnterMapOverview"
        ]
    },
